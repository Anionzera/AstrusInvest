{
  "name": "starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "concurrently \"vite\" \"cd api && python app.py\"",
    "dev:win": "powershell -Command \"Start-Process powershell -ArgumentList '-Command cd api; python app.py' -WindowStyle Normal; vite\"",
    "build": "tsc && vite build",
    "build-no-errors": "tsc && vite build",
    "build:win": "vite build",
    "electron:build:win": "npm run build:win && electron-builder build --win --publish never",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "types:supabase": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:5173 && electron electron/main.js\"",
    "electron:dev:api": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"cd api && python app.py\" \"wait-on http://localhost:5173 && electron electron/main.js\"",
    "electron:build": "npm run build && electron-builder build --win --publish never",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage"
  },
