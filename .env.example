# Chaves de API para serviços de dados de mercado
REACT_APP_ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Configurações de cache
REACT_APP_MARKET_DATA_CACHE_DURATION=24 # horas
REACT_APP_MARKET_DATA_UPDATE_INTERVAL=3600 # segundos

# Configurações de fallback
REACT_APP_USE_MOCK_DATA=false # Use dados simulados em caso de falha
REACT_APP_MOCK_DATA_REFRESH_INTERVAL=86400 # segundos (24 horas) 